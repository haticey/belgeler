[core]
	editor = vim
	repositoryformatversion = 0
	filemode = true
	bare = false
	logallrefupdates = true
	excludesfile = /home/orkung/.gitignore

[branch "master"]
	remote = origin
	merge = refs/heads/master
[web]
	browser = chromium
[color]
	ui = true
[color "branch"]
	current = yellow bold
	local = green bold
	remote = cyan bold
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
	whitespace = red reverse
[color "status"]
	added = green bold
	changed = yellow bold
	untracked = red bold
